{
  "hosting": {
    "source": ".",
    "site": "order-allnimall",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "asia-east1"
    },
    "rewrites": [ {
      "source": "**/!(*.css|*.js|*.map|*.jpg|*.gif|*.png|*.php)",
      "destination": "/index.html"
    } ]
  }
}
